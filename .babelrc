{
  "presets": [
    [
      "es2015",
      "stage-0"
    ]
  ],
  "plugins": [
    "transform-runtime",
    "transform-es3-member-expression-literals",
    "transform-es3-property-literals"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-runtime",
        "transform-es3-member-expression-literals",
        "transform-es3-property-literals"
      ]
    },
    "test": {
      "plugins": [
        [
          "istanbul",
          {
            "exclude": [
              "test/**/*.spec.js",
              "config/karma/karma.entry.js"
            ]
          }
        ]
      ]
    }
  }
}