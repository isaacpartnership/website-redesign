{
  "name": "webpack-vue-boilerplate",
  "version": "1.0.0",
  "author": "andparsons",
  "license": "MIT",
  "homepage": "https://github.com/andparsons/webpack-vue-boilerplate#readme",
  "repository": {
    "type": "git",
